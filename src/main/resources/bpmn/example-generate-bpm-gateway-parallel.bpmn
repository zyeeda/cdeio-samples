      <bpmndi:BPMNEdge bpmnElement="flow6" id="BPMNEdge_flow6">
        <omgdi:waypoint x="440.0" y="112.0"></omgdi:waypoint>
        <omgdi:waypoint x="500.0" y="112.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
