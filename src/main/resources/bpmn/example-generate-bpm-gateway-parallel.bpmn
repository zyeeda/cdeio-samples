    <startEvent id="start_event"></startEvent>
    <userTask id="user_task_01" name="人资部入职审核" activiti:candidateGroups="d-1001">
    </userTask>
    <userTask id="user_task_02" name="研发部入职审核" activiti:candidateGroups="d-1002">
    </userTask>
    <endEvent id="end_event"></endEvent>
    <sequenceFlow id="flow1" sourceRef="start_event" targetRef="parallelgateway1"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="parallelgateway1" targetRef="user_task_02"></sequenceFlow>
    <sequenceFlow id="flow3" sourceRef="parallelgateway1" targetRef="user_task_01"></sequenceFlow>
    <!-- 并行网关需要成对使用，一个用于分流(fork)，一个用于聚合(join) -->
