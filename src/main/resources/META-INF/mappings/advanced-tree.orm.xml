<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://www.oracle.com/webfolder/technetwork/jsc/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">

    <named-query name="getSubAdvancedTreeCountById">
        <query>
            select count(t) from AdvancedTree t where t.parent.id = :advancedTreeId
        </query>
    </named-query>

</entity-mappings>
